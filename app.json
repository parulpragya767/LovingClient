{
  "expo": {
    "name": "Loving",
    "slug": "loving-app",
    "scheme": "loving",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",

    "icon": "./assets/icon.png",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "bundleIdentifier": "com.loving.app",
      "buildNumber": "1.0.0",
      "supportsTablet": false
    },

    "android": {
      "package": "com.loving.app",
      "versionCode": 1
    },

    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },

    "runtimeVersion": {
      "policy": "appVersion"
    },

    "platforms": ["ios", "android", "web"],
    "sdkVersion": "54.0.0"
  }
}